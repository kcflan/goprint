{
	"name": "goprint",
	"displayName": "goprint",
	"description": "Go Print, console logs simplified",
	"repository": "https://github.com/kflan-io/goprint",
	"version": "0.0.1",
	"publisher": "kflan-io",
	"engines": {
		"vscode": "^1.60.0"
	},
	"categories": [
		"Other"
	],
	"activationEvents": [
		"onCommand:goprint.insertGoPrint"
	],
	"main": "./out/extension.js",
	"contributes": {
		"commands": [
			{
				"command": "goprint.insertGoPrint",
				"title": "insert log statement"
			}
		],
		"keybindings": [
			{
				"command": "goprint.insertGoPrint",
				"key": "shift+ctrl+i",
				"mac": "shift+cmd+i",
				"when": "editorTextFocus"
			}
		]
	},
	"scripts": {
		"vscode:prepublish": "npm run compile",
		"compile": "tsc -p ./",
		"watch": "tsc -watch -p ./",
		"pretest": "npm run compile && npm run lint",
		"lint": "eslint src --ext ts",
		"test": "node ./out/test/runTest.js"
	},
	"devDependencies": {
		"@types/glob": "^7.1.4",
		"@types/mocha": "^9.0.0",
		"@types/node": "14.x",
		"@types/vscode": "^1.60.0",
		"@typescript-eslint/eslint-plugin": "^4.31.1",
		"@typescript-eslint/parser": "^4.31.1",
		"@vscode/test-electron": "^1.6.2",
		"eslint": "^7.32.0",
		"glob": "^7.1.7",
		"mocha": "^9.1.1",
		"typescript": "^4.4.3"
	}
}
